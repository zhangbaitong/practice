{
  "id": "redis-master",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 2,
    "replicaSelector": {"name": "redisslave"},
    "podTemplate": {
      "desiredState": {
         "manifest": {
           "version": "v1beta1",
           "id": "redis-master",
           "containers": [{
             "name": "master",
             "image": "dockerfile/redis",
             "cpu": 200,
             "ports": [{"containerPort": 6379, "hostPort": 6380}]
           }]
         }
       },
       "labels": {
         "name": "redis-master",
         "uses": "redis-master",
       }
      }},
  "labels": {"name": "redis-master"}
}